package set1;

public class Q8 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a=555,h,m,s;
		h=a/3600;
		m=(a%3600)/60;
		s=(a%3600)%60;
		System.out.print("NO OF HOURS "+h+'\n');
		System.out.print("NO OF MINUTES "+m+'\n');
		System.out.print("NO OF SECONDS "+s+'\n');

	}

}
